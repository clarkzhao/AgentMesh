{
  "name": "@agentmesh/agentmesh-a2a",
  "version": "0.1.0",
  "description": "OpenClaw plugin that exposes any OpenClaw agent as a standard A2A agent",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "openclaw": {
    "extensions": [
      "./src/index.ts"
    ]
  },
  "dependencies": {
    "bonjour-service": "^1.3.0"
  },
  "peerDependencies": {
    "openclaw": ">=2026.1.0 <2027.0.0"
  },
  "peerDependenciesMeta": {
    "openclaw": {
      "optional": true
    }
  },
  "devDependencies": {
    "openclaw": "2026.2.15",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0",
    "@types/node": "^22.0.0"
  }
}
